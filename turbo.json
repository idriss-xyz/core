{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "DATABASE_URL",
    "TWITCH_CLIENT_ID",
    "TWITCH_CLIENT_SECRET",
    "TWITCH_BOT_USER_ID"
  ],
  "tasks": {
    "@idriss-xyz/main-landing#build": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "ENVIRONMENT",
        "RAILWAY_PUBLIC_DOMAIN",
        "RAILWAY_GIT_COMMIT_SHA",
        "DEV_LOGIN_PASSWORD",
        "PUBLIC_ACCESS_ENABLED",
        "CI",
        "CREATOR_LINKS_SECRET",
        "NEXT_PUBLIC_PRIVY_APP_ID",
        "FONTS_TOKEN"
      ]
    },
    "@idriss-xyz/main-landing#start": {
      "dependsOn": [
        "@idriss-xyz/main-landing#build"
      ],
      "env": [
        "ENVIRONMENT",
        "RAILWAY_PUBLIC_DOMAIN",
        "DEV_LOGIN_PASSWORD",
        "PUBLIC_ACCESS_ENABLED",
        "CI",
        "CREATOR_LINKS_SECRET",
        "NEXT_PUBLIC_PRIVY_APP_ID",
        "FONTS_TOKEN"
      ]
    },
    "@idriss-xyz/main-landing#dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "ENVIRONMENT",
        "RAILWAY_PUBLIC_DOMAIN",
        "DEV_LOGIN_PASSWORD",
        "PUBLIC_ACCESS_ENABLED",
        "CI",
        "CREATOR_LINKS_SECRET",
        "NEXT_PUBLIC_PRIVY_APP_ID",
        "FONTS_TOKEN"
      ]
    },
    "@idriss-xyz/extension#dev": {
      "cache": false,
      "env": [
        "AMPLITUDE_API_KEY",
        "ENVIRONMENT",
        "SENTRY_DSN",
        "SENTRY_ENVIRONMENT"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development",
        ".env.production"
      ]
    },
    "@idriss-xyz/extension#build": {
      "cache": true,
      "env": [
        "AMPLITUDE_API_KEY",
        "ENVIRONMENT",
        "SENTRY_DSN",
        "SENTRY_ENVIRONMENT"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development",
        ".env.production"
      ],
      "outputs": [
        "build"
      ]
    },
    "@idriss-xyz/copilot-api#dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "TOP_ADDRESSES_SECRET",
        "JWT_SECRET",
        "DATABASE_HOST",
        "DATABASE_PORT",
        "DATABASE_USER",
        "DATABASE_PASS",
        "DATABASE_DB_NAME"
      ]
    },
    "@idriss-xyz/copilot-api#prod": {
      "cache": true,
      "persistent": true,
      "env": [
        "TOP_ADDRESSES_SECRET",
        "JWT_SECRET",
        "DATABASE_HOST",
        "DATABASE_PORT",
        "DATABASE_USER",
        "DATABASE_PASS",
        "DATABASE_DB_NAME",
        "PORT",
        "HOST",
        "ALCHEMY_API_KEY",
        "API_SECRET",
        "WEBHOOK_URL",
        "VIEM_TRANSPORTER_URL",
        "HELIUS_API_KEY"
      ]
    },
    "@idriss-xyz/creator-api#dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "PORT",
        "HOST",
        "ZAPPER_API_KEY",
        "DATABASE_URL",
        "SECRET_PASSWORD",
        "ALCHEMY_API_KEY",
        "ELEVENLABS_API_KEY",
        "PRIVY_APP_ID",
        "PRIVY_PUBLIC_VERIFICATION_KEY",
        "BASE_URL",
        "JWT_SECRET",
        "TWITCH_CLIENT_ID",
        "TWITCH_CLIENT_SECRET",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_REGION",
        "AWS_S3_BUCKET",
        "AWS_LAMBDA_FAUCET_NAME",
        "AWS_LAMBDA_REWARDS_NAME",
        "SES_FROM_EMAIL",
        "FAUCET_ADDRESS",
        "OPENSEA_API_KEY",
        "TWITCH_BOT_CLIENT_ID",
        "TWITCH_BOT_CLIENT_SECRET",
        "TWITCH_BOT_USER_ID",
        "ENCRYPTION_KEY"
      ]
    },
    "@idriss-xyz/creator-api#prod": {
      "cache": true,
      "persistent": true,
      "env": [
        "PORT",
        "HOST",
        "ZAPPER_API_KEY",
        "DATABASE_URL",
        "SECRET_PASSWORD",
        "ALCHEMY_API_KEY",
        "ELEVENLABS_API_KEY",
        "PRIVY_APP_ID",
        "PRIVY_PUBLIC_VERIFICATION_KEY",
        "BASE_URL",
        "JWT_SECRET",
        "TWITCH_CLIENT_ID",
        "TWITCH_CLIENT_SECRET",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_REGION",
        "AWS_S3_BUCKET",
        "AWS_LAMBDA_FAUCET_NAME",
        "AWS_LAMBDA_REWARDS_NAME",
        "SES_FROM_EMAIL",
        "FAUCET_ADDRESS",
        "OPENSEA_API_KEY",
        "TWITCH_BOT_CLIENT_ID",
        "TWITCH_BOT_CLIENT_SECRET",
        "TWITCH_BOT_USER_ID",
        "ENCRYPTION_KEY"
      ]
    },
    "@idriss-xyz/creator-api#test": {
      "cache": false
    },
    "@idriss-xyz/twitch-extension#build": {
      "inputs": [
        "$TURBO_DEFAULT$"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "@idriss-xyz/twitch-extension#dev": {
      "cache": false,
      "persistent": true
    },
    "@idriss-xyz/ponder-indexer#dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL",
        "ZAPPER_API_KEY",
        "ALCHEMY_API_KEY",
        "INFURA_API_KEY",
        "OPENSEA_API_KEY"
      ]
    },
    "@idriss-xyz/ponder-indexer#start": {
      "cache": true,
      "persistent": true,
      "env": [
        "DATABASE_URL",
        "ZAPPER_API_KEY",
        "ALCHEMY_API_KEY",
        "INFURA_API_KEY",
        "OPENSEA_API_KEY",
        "RAILWAY_PROJECT_ID"
      ]
    },
    "lint": {
      "inputs": [
        "$TURBO_DEFAULT$",
        "../../packages/eslint-config/.eslintrc.cjs"
      ]
    },
    "lint:fix": {
      "inputs": [
        "$TURBO_DEFAULT$",
        "../../packages/eslint-config/.eslintrc.cjs"
      ]
    },
    "format": {
      "inputs": [
        "$TURBO_DEFAULT$",
        "../../prettier.config.mjs"
      ]
    },
    "format:fix": {
      "inputs": [
        "$TURBO_DEFAULT$",
        "../../prettier.config.mjs"
      ]
    },
    "unused-exports": {},
    "circular-dependencies": {},
    "type-check": {}
  }
}
